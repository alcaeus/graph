{
  "name": "alcaeus/graph",
  "description": "A graph data structure implementation in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Braun",
      "email": "work@alcaeus.org"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "squizlabs/php_codesniffer": "^3.8",
    "doctrine/coding-standard": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Alcaeus\\Graph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alcaeus\\Graph\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
